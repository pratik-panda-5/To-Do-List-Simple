<%- include('header') -%> 
    <div class="box" id="heading">
        <h1><%= today %> </h1>
    </div>
    <div class="box">
        <% for(i = 0; i<items.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= items[i] %></p>
            </div>
        <%} %> 
        <form action="/" method="post">
            <input type="text" name="newItem" placeholder="Add Item" autocomplete="off">
            <button type="submit">+</button>
        </form>
    </div>

<%- include('footer') -%>     
